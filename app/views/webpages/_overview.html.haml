#overview_tab.tab-pane.active
  .row
    .span5
      %table.table.table-condensed.table-bordered.table-hover
        %tr
          %th IP address
          %td= @webpage.primary_ip
        - if @webpage.location != ''
          %tr
            %th IP location
            %td= @webpage.location
        %tr
          %th Redirect count
          %td= @webpage_response.redirect_count