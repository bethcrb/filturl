#overview_tab.tab-pane.fade.in.active
  .col-xs-12.col-md-offset-4.col-md-4
    %table.table.table-bordered.table-hover
      %tr
        %th IP address
        %td= @webpage.primary_ip
      - if @webpage.location != ''
        %tr
          %th IP location
          %td= @webpage.location
      %tr
        %th Redirect count
        %td= @webpage_response.redirect_count
